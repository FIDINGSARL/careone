<odoo>
    <record id="expiration_date_edition_wizard" model="ir.ui.view">
        <field name="name">Expiration Date Edition</field>
        <field name="model">date.edit</field>
        <field name="arch" type="xml">
            <form string="Expiration Date Edition">
                <group>
                    <field name="expiration_date"/>
                </group>
                <footer>
                    <button string="Confirm" name="change_date" type="object"/>
                    <button string="Cancel" class="btn-default" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>